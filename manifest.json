{
    "name": "General Ad Remover",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "This extension is supposed to be used with AdBlock.",
    "author": "soobakjonmat",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
        "matches": [
            "<all_urls>"
        ],
        "js": ["src/content.js"]
        }
    ]
}